<?xml version="1.0"?>
<STM32WBUpdaterConfiguration xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
	<SupportedDeviceIDRegex>USB\\VID_0483&amp;PID_DF11($|&amp;.*)</SupportedDeviceIDRegex>
	<Version>1.5.0</Version>
	<DeviceTypes>STM32WB5xG(1M)/STM32WB5xE(512K)/STM32WB5xC(256K)</DeviceTypes>
	<Bootloaders>
		<ProgrammableBootloader>
			<FileName>stm32wb5x_FUS_fw_1_0_2.bin</FileName>
			<Version>v1.0.2</Version>
			<BaseAddress>0x080EC000/0x0807A000/0x0803A000</BaseAddress>
			<TriggerVersions>0x00050300</TriggerVersions>
		</ProgrammableBootloader>
		<ProgrammableBootloader>
			<FileName>stm32wb5x_FUS_fw.bin</FileName>
			<Version>v1.1.0</Version>
			<BaseAddress>0x080EC000/0x0807A000/0x0803A000</BaseAddress>
			<TriggerVersions>0x01000100/0x01000200</TriggerVersions>
		</ProgrammableBootloader>
		<ProgrammableBootloader>
			<Version>(up-to-date)</Version>
			<TriggerVersions>0x1010000</TriggerVersions>
		</ProgrammableBootloader>
	</Bootloaders>
	<Stacks>
		<ProgrammableBinary>
			<Line1>HCI Layer only mode 5.0 certified : Link Layer, HCI</Line1>
			<FileName>stm32wb5x_BLE_HCILayer_fw.bin</FileName>
			<Version>v1.5.0</Version>
			<BaseAddress>0x080DC000/0x08068000/0x08028000</BaseAddress>
		</ProgrammableBinary>
		<ProgrammableBinary>
			<Line1>Full BLE Stack 5.0 certified : Link Layer, HCI, L2CAP, ATT, SM, GAP and GATT database</Line1>
			<FileName>stm32wb5x_BLE_Stack_full_fw.bin</FileName>
			<Version>v1.5.0</Version>
			<BaseAddress>0x080CB000/0x08057000/0x08017000</BaseAddress>
		</ProgrammableBinary>
		<ProgrammableBinary>
			<Line1>Wireless Ble stack Light configuration â€“ Slave Only</Line1>
			<FileName>stm32wb5x_BLE_Stack_light_fw.bin</FileName>
			<Version>v1.5.0</Version>
			<BaseAddress>0x80D9000/0x8065000/0x8025000</BaseAddress>
		</ProgrammableBinary>
		<ProgrammableBinary>
			<Line1>Static Concurrent Mode BLE Thread</Line1>
			<Line2>Supports Full BLE Stack 5.0 certified and Full Thread Device certified</Line2>
			<FileName>stm32wb5x_BLE_Thread_fw.bin</FileName>
			<Version>v1.5.0</Version>
			<BaseAddress>0x08078000/-/-</BaseAddress>
		</ProgrammableBinary>
		<ProgrammableBinary>
			<Line1>Static Concurrent Mode BLE Zigbee</Line1>
			<Line2>Supports Full BLE Stack 5.0 certified and Zigbee FFD</Line2>
			<FileName>stm32wb5x_BLE_Zigbee_FFD_static_fw.bin</FileName>
			<Version>v1.5.0</Version>
			<BaseAddress>0x0807C000/-/-</BaseAddress>
		</ProgrammableBinary>

		<ProgrammableBinary>
			<Line1>MAC API for IEEE 802.15.4-201</Line1>
			<Line2>To be used for MAC FFD and RFD devices</Line2>
			<FileName>stm32wb5x_Mac_802_15_4_fw.bin</FileName>
			<Version>v1.5.0</Version>
			<BaseAddress>0x080E4000/0x08070000/0x08030000</BaseAddress>
		</ProgrammableBinary>
		
		<ProgrammableBinary>
			<Line1>Dedicated firmware binary to be used with STM32CubeMonitor-RF application.</Line1>
			<Line2>Refer to STM32CubeMonitor-RF User Manual (UM2288) to get application details.</Line2>
			<FileName>stm32wb5x_rfmonitor_phy802_15_4_fw.bin</FileName>
			<Version>v1.1.0</Version>
			<BaseAddress>0x080EC000/0x08078000/0x08038000</BaseAddress>
		</ProgrammableBinary>
		
		<ProgrammableBinary>
			<Line1>Full Thread Device certified</Line1>
			<Line2>To be used for Leader / Router / End Device Thread role (full features except Border Router)</Line2>
			<FileName>stm32wb5x_Thread_FTD_fw.bin</FileName>
			<Version>v1.5.0</Version>
			<BaseAddress>0x0809F000/0x0802B000/-</BaseAddress>
		</ProgrammableBinary>
		<ProgrammableBinary>
			<Line1>Minimal Thread Device certified</Line1>
			<Line2>To be used for End Device and Sleepy End Device Thread role</Line2>
			<FileName>stm32wb5x_Thread_MTD_fw.bin</FileName>
			<Version>v1.5.0</Version>
			<BaseAddress>0x080B4000/0x08040000/-</BaseAddress>
		</ProgrammableBinary>
		<ProgrammableBinary>
			<Line1>Zigbee Compliant Platform certified</Line1>
			<Line2>Supports Full Function Device (FFD)</Line2>
			<FileName>stm32wb5x_Zigbee_FFD_Full_fw.bin</FileName>
			<Version>v1.5.0</Version>
			<BaseAddress>0x080A9000/0x08035000/-</BaseAddress>
		</ProgrammableBinary>

	</Stacks>
</STM32WBUpdaterConfiguration>