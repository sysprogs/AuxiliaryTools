<?xml version="1.0"?>
<STM32WBUpdaterConfiguration xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
	<SupportedDeviceIDRegex>USB\\VID_0483&amp;PID_DF11($|&amp;.*)</SupportedDeviceIDRegex>
	<Version>1.0</Version>
	<Bootloader>
		<Line1>Firmware Upgrade Services Binary</Line1>
		<Line2>Firmware Upgrade Services Binary</Line2>
		<FileName>stm32wb5x_FUS_fw.bin</FileName>
		<Version>v1.0.1</Version>
		<BaseAddress>0x080EC000</BaseAddress>
	</Bootloader>
	<ExpectedBootloaderVersion>0x01000100</ExpectedBootloaderVersion>
	<Stacks>
		<ProgrammableBinary>
			<Line1>Full BLE Stack 5.0 certified : Link Layer, HCI, L2CAP, ATT, SM, GAP and GATT database</Line1>
			<FileName>stm32wb5x_BLE_Stack_fw.bin</FileName>
			<Version>v1.1.0</Version>
			<BaseAddress>0x080CC000</BaseAddress>
		</ProgrammableBinary>
		<ProgrammableBinary>
			<Line1>HCI Layer only mode 5.0 certified : Link Layer, HCI</Line1>
			<FileName>stm32wb5x_BLE_HCILayer_fw.bin</FileName>
			<Version>v1.1.0</Version>
			<BaseAddress>0x080DC000</BaseAddress>
		</ProgrammableBinary>
		<ProgrammableBinary>
			<Line1>Full Thread Device certified</Line1>
			<Line2>To be used for Leader / Router / End Device Thread role (full features except Border Router)</Line2>
			<FileName>stm32wb5x_Thread_FTD_fw.bin</FileName>
			<Version>v1.1.0</Version>
			<BaseAddress>0x0809F000</BaseAddress>
		</ProgrammableBinary>
		<ProgrammableBinary>
			<Line1>Minimal Thread Device certified</Line1>
			<Line2>To be used for End Device and Sleepy End Device Thread role</Line2>
			<FileName>stm32wb5x_Thread_MTD_fw.bin</FileName>
			<Version>v1.1.0</Version>
			<BaseAddress>0x080B5000</BaseAddress>
		</ProgrammableBinary>
		<ProgrammableBinary>
			<Line1>Static Concurrent Mode BLE Thread</Line1>
			<Line2>Supports Full BLE Stack 5.0 certified and Full Thread Device certified</Line2>
			<FileName>stm32wb5x_BLE_Thread_fw.bin</FileName>
			<Version>v1.1.0</Version>
			<BaseAddress>0x08079000</BaseAddress>
		</ProgrammableBinary>
		<ProgrammableBinary>
			<Line1>MAC API for IEEE 802.15.4-201</Line1>
			<Line2>To be used for MAC FFD and RFD devices</Line2>
			<FileName>stm32wb5x_Mac_802_15_4_fw.bin</FileName>
			<Version>v1.1.0</Version>
			<BaseAddress>0x080E6000</BaseAddress>
		</ProgrammableBinary>
		<ProgrammableBinary>
			<Line1>Dedicated firmware binary to be used with STM32CubeMonitor-RF application.</Line1>
			<Line2>Refer to STM32CubeMonitor-RF User Manual (UM2288) to get application details.</Line2>
			<FileName>stm32wb5x_rfmonitor_phy802_15_4_fw.bin</FileName>
			<Version>v1.1.0</Version>
			<BaseAddress>0x080EC000</BaseAddress>
		</ProgrammableBinary>
	</Stacks>
</STM32WBUpdaterConfiguration>